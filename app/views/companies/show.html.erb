          <tr> 
            <td><b><%= I18n.t :city %></b></th>
            <td><%= @company.city %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :state %></b></th>
            <td><%= @company.state %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :country %></b></th>
            <td><%= @company.country %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :site %></b></th>
            <td><%= @company.site %></td>            
			    </tr>
        </table>
      </div>
    </div>

    <footer>
      <div class="submit_link">
        <%= link_to image_tag("/assets/icn_edit_article.png", :alt => I18n.t(:edit), :title => I18n.t(:edit)), edit_company_path(@company) %> 
        <%= link_to image_tag("/assets/icn_jump_back.png", :alt => I18n.t(:back), :title => I18n.t(:back)), companies_path %>
      </div>
    </footer>
  </article>
