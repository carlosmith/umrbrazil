  <%= outgoing_menssage %>
  <article class="module width_full">
    <header><h3><%= t :showing %></h3></header>
    <div class="tab_container">
      <div id="tab1" class="tab_content">
        <table class="tablesorter" cellspacing="0">
          <tr>
            <td><b><%= t :name %></b></th>
            <td><%= @company.name %></td>
          </tr>
          <tr>
            <td><b><%= t :brand_name %></b></th>
            <td><%= @company.brand_name %></td>
	  </tr>
          <tr>
            <td><b><%= t :cnpj %></b></th>
            <td><%= @company.cnpj %></td>
	  </tr>
          <tr>
            <td><b><%= t :state_inscription %></b></th>
            <td><%= @company.state_inscription %></td>
	  </tr>
          <tr>
            <td><b><%= t :municipal_inscription %></b></th>
            <td><%= @company.municipal_inscription %></td>
	  </tr>
          <tr>
            <td><b><%= t :address %></b></th>
            <td><%= @company.address %></td>
	  </tr>
          <tr>
            <td><b><%= t :number %></b></th>
            <td><%= @company.number %></td>
	  </tr>
          <tr>
            <td><b><%= t :complement %></b></th>
            <td><%= @company.complement %></td>
	  </tr>
          <tr>
            <td><b><%= t :neighborhood %></b></th>
            <td><%= @company.neighborhood %></td>
	  </tr>
          <tr>
            <td><b><%= t :city %></b></th>
            <td><%= @company.city %></td>
	  </tr>
          <tr>
            <td><b><%= t :state %></b></th>
            <td><%= @company.state %></td>
	  </tr>
          <tr>
            <td><b><%= t :country %></b></th>
            <td><%= @company.country %></td>
	  </tr>
          <tr>
            <td><b><%= t :site %></b></th>
            <td><%= @company.site %></td>
	  </tr>
        </table>
      </div>
    </div>

    <footer>
      <div class="submit_link">
        <%= link_to image_tag("/assets/icn_edit_article.png", alt: t(:edit), title: t(:edit)), edit_company_path(@company) %>
        <%= link_to image_tag("/assets/icn_jump_back.png", alt: t(:back), title: t(:back)), companies_path %>
      </div>
    </footer>
  </article>
