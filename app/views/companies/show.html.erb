		  <div id="tab1" class="tab_content">
			  <table class="tablesorter" cellspacing="0">
          <tr> 
            <td><b><%= I18n.t :name %></b></th>
            <td><%= @company.name %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :brand_name %></b></th>
            <td><%= @company.brand_name %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :cnpj %></b></th>
            <td><%= @company.cnpj %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :state_inscription %></b></th>
            <td><%= @company.state_inscription %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :municipal_inscription %></b></th>
            <td><%= @company.municipal_inscription %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :address %></b></th>
            <td><%= @company.address %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :number %></b></th>
            <td><%= @company.number %></td>            
			    </tr>
          <tr> 
            <td><b><%= I18n.t :complement %></b></th>
